{
  "name": "notion-workspace",
  "version": "19.1.7",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:hmr": "NG_HMR_TEMPLATES=1 ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "prepare": "husky",
    "release": "standard-version",
    "release:ci": "standard-version --no-verify --no-commit-hooks",
    "ngx-notion-cms:lint": "ng lint ngx-notion-cms",
    "ngx-notion-cms:test": "ng test ngx-notion-cms --no-watch --no-progress --browsers=ChromeHeadless",
    "ngx-notion-cms:build": "ng build ngx-notion-cms --configuration production",
    "ngx-notion-cms:tailwind": "npx tailwindcss -c projects/ngx-notion-cms/tailwind.config.js -i projects/ngx-notion-cms/src/styles.css -o dist/ngx-notion-cms/styles.css",
    "ngx-notion-cms:publish": "npm run ngx-notion-cms:lint && npm run ngx-notion-cms:build && npm run ngx-notion-cms:tailwind && npm publish dist/ngx-notion-cms/",
    "serve:ssr:ngx-notion-testbed-app": "node dist/ngx-notion-testbed-app/server/server.mjs",
    "build:prerender:ngx-notion-testbed-app": "npm run prerender && ng build ngx-notion-testbed-app",
    "prepublishOnly": "npm run lint && npm run test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.15",
    "@angular/common": "^20.3.15",
    "@angular/compiler": "^20.3.15",
    "@angular/core": "^20.3.15",
    "@angular/forms": "^20.3.15",
    "@angular/platform-browser": "^20.3.15",
    "@angular/platform-browser-dynamic": "^20.3.15",
    "@angular/platform-server": "^20.3.15",
    "@angular/router": "^20.3.15",
    "@angular/ssr": "^20.3.13",
    "express": "^4.18.2",
    "ngx-highlightjs": "^12.0.0",
    "ngx-notion-cms": "file:dist/ngx-notion-cms",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.13",
    "@angular/cli": "^20.3.13",
    "@angular/compiler-cli": "^20.3.15",
    "@eslint/js": "^9.11.1",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "angular-eslint": "18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.1",
    "globals": "^15.10.0",
    "husky": "^8.0.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.3.2",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.50.0"
  }
}